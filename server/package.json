{
    "name": "mongo-atlas",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "mongodb": "^3.6.0",
        "mongoose": "^5.10.2"
    },
    "devDependencies": {
        "@types/concurrently": "^5.2.1",
        "@types/cors": "^2.8.7",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.7",
        "@types/jest": "^26.0.10",
        "@types/mongodb": "^3.5.26",
        "@types/mongoose": "^5.7.36",
        "@types/node": "^14.6.2",
        "concurrently": "^5.3.0",
        "debug": "^4.1.1",
        "jest": "^26.4.2",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.1",
        "ts-jest": "^26.3.0",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    }
}
